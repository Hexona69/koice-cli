{
  "name": "koice.js-binary",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc && copyfiles -u 1 \"src/**/*\" -e \"src/**/*.ts\" dist",
    "clean": "rimraf dist/*",
    "make": "npm run clean && npm run compile",
    "build-clean": "rimraf build/*",
    "build-darwin": "nexe dist/index.js --resource \"./node_modules/**/*\" --target mac --fake-argv --build --output build/koice-darwin-amd64",
    "build-windows": "nexe dist/index.js --resource \"./node_modules/**/*\" --target windows --fake-argv --build --output build/koice-windows-amd64",
    "build-linux": "nexe dist/index.js --resource \"./node_modules/**/*\" --target linux --fake-argv --build --output build/koice-linux-amd64",
    "build": "npm run make && npm run build-darwin && npm run build-windows && npm run build-linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hexona69/koice.js-binary.git"
  },
  "author": "Hexona",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hexona69/koice.js-binary/issues"
  },
  "homepage": "https://github.com/Hexona69/koice.js-binary#readme",
  "dependencies": {
    "argparse": "^2.0.1",
    "delay": "^5.0.0",
    "koice": "^0.1.11",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/node": "^18.11.17",
    "@types/readline-sync": "^1.4.4"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node16-macos-x64"
    ],
    "outputPath": "build"
  }
}